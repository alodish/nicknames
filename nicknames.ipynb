     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "Acer = Acera\n",
      "6\n",
      "..."
      "801\n",
      "Zeph = Zephaniah \n",
     ]
    }
   ],
   "source": [
    "lines = [s for s in soup.strings if '=' in s]\n",
    "k = 0\n",
    "for line in lines:\n",
    "    if '=' in line:\n",
    "        print(k)\n",
    "        k += 1\n",
    "        print(line)"
   ]
  },
